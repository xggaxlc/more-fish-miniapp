{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "experimentalDecorators": true,
    "module": "commonjs",
    "lib": ["dom", "esnext", "esnext.asynciterable"],
    "allowJs":true,
    "baseUrl": "./",
    "paths": {
      "@store": ["src/store/index"],
      "@store/*": ["src/store/*"],
      "@utils": ["src/utils/index"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "include": [
    "src/**/*"
  ]
}
